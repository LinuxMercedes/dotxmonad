#!/bin/bash

#enable ctrl-alt-backspace
setxkbmap -option terminate:ctrl_alt_bksp

xrdb -merge .Xresources

#wallpaper
feh --bg-fill /home/nathan/Pictures/Wallpapers/Fractals\ and\ abstract\ stuff/1211833264722.jpg &

xscreensaver -no-splash&

trayer --edge top --align right --SetDockType true --expand true --width 15 --height 18 --transparent true --alpha 0 --tint 0x000000 &

wicd-gtk --tray &

udiskie &

exec xmonad

